{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/serviceWorker.js",
      "src/vendor/**/*"
    ]
  },
  "rules": {
    "no-console" : false,
    "object-literal-sort-keys" : false,
    "variable-name" : [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
